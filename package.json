{
  "name": "vstofolder",
  "publisher": "bit4bit",
  "displayName": "Find Folder",
  "description": "Instantly find and navigate to any folder in your workspace.",
  "version": "1.2.2",
  "license": "MIT",
  "engines": {
    "vscode": "^1.99.0",
    "windsurf": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bit4bit/vstofolder.git"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "folder",
    "navigation",
    "workspace",
    "find",
    "explorer",
    "windsurf",
    "vscode"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vstofolder.FindFolder",
        "title": "Find Folder"
      }
    ],
    "configuration": {
      "title": "VS To Folder",
      "properties": {
        "vstofolder.cacheValidationDuration": {
          "type": "number",
          "default": 60,
          "minimum": 0,
          "maximum": 60,
          "description": "Cache validation duration in minutes. Set to 0 to disable caching."
        },
        "vstofolder.enableDetailedLogging": {
          "type": "boolean",
          "default": false,
          "description": "Enable detailed logging for debugging purposes (affects performance)."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.99.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "0.25.9",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2"
  }
}
